{
    "name": "themegazord/capyui",
    "description": "CapyUI is a modern, modular UI components library for Laravel applications, focused on clean design and usability. Inspired by the capybara - a symbol of calmness and adaptability - CapyUI makes it easy to build elegant, consistent interfaces quickly and effortlessly.",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Capy\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gustavo de Camargo Campos",
            "email": "contato.wanjalagus@outlook.com.br",
            "homepage": "https://github.com/themegazord"
        }
    ],
    "keywords": [
        "laravel",
        "livewire",
        "livewire 3",
        "livewire ui",
        "livewire ui components",
        "tallstack",
        "tallstack ui",
        "tallstackui",
        "tallstack components",
        "blade",
        "ui",
        "blade ui components",
        "livewire-components",
        "livewire components",
        "livewire-packages",
        "livewire packages",
        "tailwind",
        "components",
        "alpinejs"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Capy\\CapyServiceProvider"
            ]
        }
    }
}
